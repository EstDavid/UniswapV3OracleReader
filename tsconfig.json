{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node 14",

  "files": [
    "./src/uniswapV3Oracle.ts"
  ],
  
  "compilerOptions": {
    "lib": ["es2018"],
    "module": "commonjs",
    "target": "es2018",
    "rootDir": "./",
    "outDir": "./",
    
    "resolveJsonModule": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },

  "exclude": [
    "node_modules"
  ]
}